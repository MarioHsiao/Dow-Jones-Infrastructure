				<url lang="ja">http://global.beta.factiva.com/ja/cfg/pswdReset1.asp</url>
				<url lang="ru">http://global.beta.factiva.com/ru/cfg/pswdReset1.asp</url>
				<url lang="zhcn">http://global.beta.factiva.com/zhcn/cfg/pswdReset1.asp</url>
				<url lang="zhtw">http://global.beta.factiva.com/zhtw/cfg/pswdReset1.asp</url>
				<url lang="_${_LA}">http://global.beta.factiva.com/templates/cfg/pswdReset1.asp</url>
				<url lang="templates">http://global.beta.factiva.com/templates/cfg/pswdReset1.asp</url>
			</template>
			<template type="psswd_reset2" enabled="true">
				<url lang="en">http://global.beta.factiva.com/en/cfg/pswdReset2.asp</url>
				<url lang="fr">http://global.beta.factiva.com/fr/cfg/pswdReset2.asp</url>
				<url lang="de">http://global.beta.factiva.com/de/cfg/pswdReset2.asp</url>
				<url lang="es">http://global.beta.factiva.com/es/cfg/pswdReset2.asp</url>
				<url lang="it">http://global.beta.factiva.com/it/cfg/pswdReset2.asp</url>
				<url lang="ja">http://global.beta.factiva.com/ja/cfg/pswdReset2.asp</url>
				<url lang="ru">http://global.beta.factiva.com/ru/cfg/pswdReset2.asp</url>
				<url lang="zhcn">http://global.beta.factiva.com/zhcn/cfg/pswdReset2.asp</url>
				<url lang="zhtw">http://global.beta.factiva.com/zhtw/cfg/pswdReset2.asp</url>
				<url lang="_${_LA}">http://global.beta.factiva.com/templates/cfg/pswdReset2.asp</url>
				<url lang="templates">http://global.beta.factiva.com/templates/cfg/pswdReset2.asp</url>
			</template>
			<data for="jumppage">
				<url lang="en">http://global.beta.factiva.com/en/cfg/jumppage.asp?billpack=WSJ</url>
			</data>
			<redirect type="logout" enabled="true"></redirect>
			
			<browsercaps>
				<use var="HTTP_USER_AGENT"/>
				<keys>
					<key name="platform" value="Unknown" />
					<key name="browser" value="Unknown" />
					<key name="version" value="0.0" />
					<key name="majorversion" value="0" />
					<key name="minorversion" value="0" />
					<key name="beta" value="false" />
					<key name="win16" value="false" />
					<key name="win32" value="false" />
					<key name="supportedPlatform" value="false" />
					<key name="supportedBrowser" value="false" />
					<key name="letBrowserIn" value="false" />
				</keys>
				<filter>
					<case match="Windows 3.1|Win16"><key name="platform" value="Win16" /></case>
					<case match="Windows 95|Win95"><key name="platform" value="Win95" /></case>
					<case match="Windows 98|Win98"><key name="platform" value="Win98" /></case>
					<case match="Windows NT|WinNT"><key name="platform" value="WinNT" /></case>
					<case match="Windows 2000|Win2000"><key name="platform" value="Win2000" /></case>
					<case match="Windows CE|WinCE"><key name="platform" value="WinCE" /></case>
					<case match="Windows ME|WinME"><key name="platform" value="WinME" /></case>
					<case match="Mac_68000|Macintosh.*68K"><key name="platform" value="Mac68K" /></case>
					<case match="Mac_PowerPC|Macintosh.*PPC"><key name="platform" value="MacPPC" /></case>
					<case match="X11"><key name="platform" value="Unix" /></case>
					<case match="WebTV"><key name="platform" value="WebTV" /></case>
				</filter>
				<filter>
					<case match="16bit|Windows 3.1|Win16">
						<key name="win16" value="true" />
					</case>
					<case match="Windows 95|Win95|Windows 98|Win98|Windows NT|WinNT|Win32">
						<key name="win32" value="true" />
					</case>
				</filter>
				<filter>
					<case match="Opera[ /]((\d+)\.(\d+)(\w*))">
						<key name="browser" value="Opera" />
						<key name="version" value="${1}" />
						<key name="majorversion" value="${2}" />
						<key name="minorversion" value="${3}" />
						<filter match="^b" with="${4}">
							<key name="beta" value="true" />
						</filter>
					</case>
					<case match="^Mozilla[^(]*\(compatible; MSIE\s+((\d+)\.(\d+)(\w*))(.*)">
						<key name="browser" value="IE" />
						<key name="version" value="${1}" />
						<key name="majorversion" value="${2}" />
						<key name="minorversion" value="${3}" />
						<case match="^[5-9]\." with="${1}">
							<filter with="${4}" match="^b"><key name="beta" value="true" /></filter>
						</case>
						<case match="^4\." with="${1}">
				             <filter with="${4}" match="^[ab]"><key name="beta" value="true" /></filter>     
				        </case>
						<case match="^3\." with="${1}">
				             <filter match="true" with="%{win16}">
				                 <filter match="^a" with="${4}"><key name="beta" value="true" /></filter>
				              </filter>
				            <filter match="^B" with="${4}"><key name="beta" value="true" /></filter>
				        </case>
				        <case match="^2\." with="${1}">
				           <filter match="^b" with="${4}"><key name="beta" value="true" /></filter>
				        </case>
					</case>
					
					<case match="^Mozilla/\d+\.\d+ \(.[^)]*\) Gecko/\d+ Netscape[^/]*/((\d+)\.((\.?\d+)*))(\w*)">
						<key name="browser" value="Netscape" />
						<key name="version" value="${1}" />
						<key name="majorversion" value="${2}" />
						<key name="minorversion" value="${3}" />
						<filter match="^b" with="${5}"><key name="beta" value="true" /></filter>
					</case>
					<case match="^Mozilla/((\d+)\.(\d+)(\w*)).*">
						<key name="browser" value="Mozilla" />
						<key name="version" value="${1}" />
						<key name="majorversion" value="${2}" />
						<key name="minorversion" value="${3}" />
						<filter match="^b" with="${4}"><key name="beta" value="true" /></filter>
					</case>
				</filter>
				<filter with="%{platform}" match="Win16|Win95|Win98|WinNT|Win2000|WinCE|WinME|Mac68K|MacPPC">
					<key name="supportedPlatform" value="true" />
				</filter>
				<filter>
          <case match="^IE\b" with="%{browser}">
            <case match="false" with="%{beta}">
              <case match="^[5-9]\b" with="%{majorversion}">
                <case match="Mac68K|MacPPC" with="%{platform}">
                  <key name="actId" value="[{_A}]"/>
                  <case match="9PRO000200|9FAC000700" with="%{actId}">
                    <key name="supportedBrowser" value="true"/>
                  </case>
                </case>
                <case match="Win16|Win95|Win98|WinNT|Win2000|WinCE|WinME" with="%{platform}">
                  <case match="^5\b" with="%{majorversion}">
                    <case match="^(0)*[2-9](\d)*" with="%{minorversion}">
                      <key name="supportedBrowser" value="true"/>
                      <case match="^(0)*[2-5](\d)*" with="%{minorversion}">
                        <key name="sunSettedBrowser" value="true"/>
                      </case>
                    </case>
                  </case>
                  <case match="(\d)*" with="%{minorversion}">
                    <key name="supportedBrowser" value="true"/>
                  </case>
                </case>
              </case>
            </case>
          </case>
            <case match="^Netscape" with="%{browser}">
              <case match="^[6-9]\b" with="%{majorversion}">
                <key name="supportedBrowser" value="true"/>
              </case>
            </case>
            <case match="^Mozilla" with="%{browser}">
              <case match="^4\b" with="%{majorversion}">
                <case match="^(0)*[7-9](\d)*" with="%{minorversion}">
                  <key name="letBrowserIn" value="true"/>
                </case>
              </case>
              <case match="^[5-9]\b" with="%{majorversion}">
							<key name="letBrowserIn" value="true" />
						</case>
					</case>
				</filter>
			</browsercaps>
		</product>
	</products>
</configuration>