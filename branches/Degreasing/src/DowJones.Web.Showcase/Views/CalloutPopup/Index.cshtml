@{
    ViewBag.Title = "Callout Popup";
}
<script type="text/javascript">
    $(document).ready(function () {

        $('#fc').click(function () {
            $dj.callout({
                title: 'Trend Title...',
                close: function () { },
                trig: $(this),
                show: function () {
                    setTimeout(function () {
                        body = $('#trend-down-example').html();
                        $dj.updateCalloutContent({ body: body });
                    }, 1000);

                }
            });
        });

        $('#md').click(function () {
            $dj.callout({
                title: '',
                height:150,
                close: function () { },
                trig: $(this),
                show: function () {
                    setTimeout(function () {
                        body= $('#person-example').html(),
                       $dj.updateCalloutContent({ body: body });
                    }, 1000);

                }
            });
        });

        $('#sc').click(function () {
            $dj.callout({
                title: 'Compnay Title',
                close: function () { },
                trig: $(this),
                show: function () {
                    setTimeout(function () {
                        body= $('#company-example').html(),
                       $dj.updateCalloutContent({ body: body });
                    }, 1000);

                }
            });
            return false;
        });

    });


