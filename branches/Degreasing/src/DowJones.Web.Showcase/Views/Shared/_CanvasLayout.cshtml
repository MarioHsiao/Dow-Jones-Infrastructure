@{
    Layout = null;

    DJ.ScriptRegistry()
        .WithJQueryUIInteractions()
        .WithJQueryUIEffects();

    DJ.StylesheetRegistry()
        .Include("~/Styles/css/GlobalLayout.min.css")
        .Include("~/Styles/css/NavBar.css");
    
    ViewBag.Title = "Showcase Dashboard";
}
<!doctype html>
<!--[if lt IE 7 ]> <html lang="en" class="no-js ie6"> <![endif]-->
<!--[if IE 7 ]>    <html lang="en" class="no-js ie7"> <![endif]-->
<!--[if IE 8 ]>    <html lang="en" class="no-js ie8"> <![endif]-->
<!--[if IE 9 ]>    <html lang="en" class="no-js ie9"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html lang="en" class="no-js">
<!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>@ViewBag.Title</title>
    <link rel="icon" type="image/png" href="@Url.Content("~/areas/factiva/content/ux-temp/images/favicon.png")">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <script type="text/javascript">
        var DJ = { config: { debug: true} };
    </script>
    
    @Html.DJ().ScriptRegistry().RenderHead()
    @Html.DJ().StylesheetRegistry().RenderHead()
    
    <script type="text/javascript">
        var $dj = DJ.$dj;
    </script>
</head>
<body>
    <div class="message">
        <div class="message-wrap container_9">
            <p class="message-text">
                @Html.DJ().Token("WelcomeToNewNewspages")
                <a href="./Index_files/Index.htm" title="@Html.DJ().Token("ReturnToOldNewspages")">Factiva.com
                    <span class="fi fi_r-arrow"></span></a>
            </p>
        </div>
    </div>
    <!-- end: .message -->
    <div id="header" class="header">
        <div id="header-top">
            <div id="header-top-wrap" class="container_9 clearfix">
                <ul id="welcome-nav" class="clearfix">
                    <li class="first"><a href="./Index_files/Index.htm" title="@Html.DJ().Token("Welcome"), John Smith!">@Html.DJ().Token("Welcome"),
                        John Smith!</a></li>
                </ul>
                <!-- end: #welcome-nav -->
                <ul id="user-nav" class="clearfix">
                    <li class="first"><a href="./Index_files/Index.htm" title="@Html.DJ().Token("PreferencesSubtext")">@Html.DJ().Token("Preferences")</a></li>
                    <li><a href="#" title="Support">Support</a></li>
                    <li class="last"><a href="./Index_files/Index.htm" title="@Html.DJ().Token("LogoutSubtext")">@Html.DJ().Token("Logout")</a></li>
                </ul>
                <!-- end: #user-nav -->
            </div>
        </div>
        <!-- end: #header-top -->
        <div id="header-bottom">
            <div id="header-bottom-wrap" class="container_9">
                <div id="header-content" class="clearfix">
                    <div id="product-logo">
                        <h1 class="png_bg">
                            Factiva</h1>
                    </div>
                    <!-- end: #product-logo -->
                    <div id="header-search">
                        <form action="./Index_files/Index.htm" method="post" class="clearfix">
                        <div class="form-field input">
                            <input type="text" name="s" id="" value="Search companies, industries, topics, people, documents or news"
                                class="search-field default" />
                        </div>
                        <div class="submit-btn-wrap">
                            <input type="submit" id="" class="s-btn" value="Search" />
                        </div>
                        </form>
                    </div>
                    <!-- end: #factiva-search -->
                </div>
                <!-- end: #header-search -->
            </div>
        </div>
        <!-- end: #header-bottom -->
    </div>
    <nav>
    @DJ.Render(ViewBag.NavBar)
    </nav>
    <!-- end: #header -->

    @RenderBody()

    <!-- end: #dashboard -->
    <div id="footer">
        <div id="footer-top">
            <div id="footer-top-wrap" class="container_9 clearfix">
                <div id="product-logo">
                    <h1 class="png_bg">
                        Factiva</h1>
                </div>
                <!-- end: #client-logo -->
                <div id="footer-search">
                    <form action="./Index_files/Index.htm" method="post">
                    <div class="form-field input">
                        <input type="text" name="s" id="" value="Search companies, industries, topics, people, documents or news"
                            class="search-field default" />
                    </div>
                    <div class="submit-btn-wrap">
                        <input type="submit" id="" class="s-btn" value="Search" />
                    </div>
                    </form>
                </div>
                <!-- end: #factiva-search -->
            </div>
        </div>
        <!-- DEV: The footer-links markup remains in place though the content is hidden from the browser.  -->
        <div id="footer-links">
            <div id="footer-links-wrap" class="container_9 clearfix">
                <div id="industry-links">
                    <h5 class="section-title">
                        Industry</h5>
                    <div id="industry-links-wrap" class="clearfix">
                        <ul class="footer-links-list">
                            <li></li>
                        </ul>
                        <ul class="footer-links-list">
                            <li></li>
                        </ul>
                        <ul class="footer-links-list">
                            <li></li>
                        </ul>
                        <ul class="footer-links-list">
                            <li></li>
                        </ul>
                    </div>
                </div>
                <div id="ma-legal-links">
                    <div id="my-account-links">
                        <h5 class="section-title">
                            My Account</h5>
                        <ul class="footer-links-list">
                            <li><a href="./Index_files/Index.htm">Profile</a></li>
                            <li><a href="./Index_files/Index.htm">Preferences</a></li>
                        </ul>
                    </div>
                    <div id="legal-links">
                        <h5 class="section-title">
                            My Account</h5>
                        <ul class="footer-links-list">
                            <li><a href="./Index_files/Index.htm">Feedback</a></li>
                            <li><a href="./Index_files/Index.htm">Privacy Policy</a></li>
                            <li><a href="./Index_files/Index.htm">Contact Us</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div id="copyright">
            <div id="copyright-wrap" class="container_9 clearfix">
                <p class="copyright-text">
                    © 2011 Dow Jones Factiva | All Rights Reserved</p>
                <p class="copyright-text-links">
                    <a href="#">Feedback</a> | <a href="#">What's New</a> | <a href="#">Privacy Policy</a>
                    | <a href="#">Contact Us</a></p>
            </div>
        </div>
        <!-- end: .copyright -->
    </div>

    @DJ.ScriptRegistry().Render()
</body>
</html>
