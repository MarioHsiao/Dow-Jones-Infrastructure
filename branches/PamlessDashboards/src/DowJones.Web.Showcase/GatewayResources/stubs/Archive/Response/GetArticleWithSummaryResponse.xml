      <AccessionNo value="METBNA0020090106e5160025u" fid="an"></AccessionNo>
      <Headline fid='hd'>
        <Para lang='en'>Codelco to get $1 billion investment handout</Para>
      </Headline>
      <Byline index='field' fid='by'>Gideon Long   </Byline>
      <Credit index='field' fid='cr'></Credit>
      <Num fid='wc' value='197'/>
      <Date value="20090106" fid="pd"></Date>
      <Time value="174900" fid="et"></Time>
      <SrcName fid="sn">Metal Bulletin News Alert Service</SrcName>
