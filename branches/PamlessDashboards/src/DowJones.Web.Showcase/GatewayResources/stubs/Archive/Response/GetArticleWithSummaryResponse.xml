      <Headline fid='hd'>
        <Para lang='en'>US Gasoline Demand -1.8% Vs Week Ago - SpendingPulse</Para>
      </Headline>
      <Byline index='field' fid='by'></Byline>
      <Credit index='field' fid='cr'></Credit>
      <Num fid='wc' value='425'/>
      <Date value="20090106" fid="pd"></Date>
