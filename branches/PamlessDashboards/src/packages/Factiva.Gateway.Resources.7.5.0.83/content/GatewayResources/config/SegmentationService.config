<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="ServiceSettings" type="Factiva.Gateway.Config.V1_0.ServiceSectionHandler, FactivaGateway"/>
	</configSections>
	<ServiceSettings>
		<transactionSettings>
			<!-- RTS Gateway GetSum-->
			<transaction key="Factiva.Gateway.Messages.Segmentation.RTSGateway.V1_0:GetSum">
				<name>TRK_SEG_SAMPLE</name>
				<request>
					<messageFormat>STRING</messageFormat>
				</request>
				<response>
					<messageFormat>STRING</messageFormat>
				</response>
				<transport type="RTS"/><transport type="HTTP"/>
			</transaction>

			<!-- CSPNET GetSum-->
			<transaction key="Factiva.Gateway.Messages.Segmentation.CSPNET.V1_0:GetSum">
				<name>urn:dowjones:segSample:v1_0:GetSum_bstr</name>
				<request>
					<messageFormat>STRING</messageFormat>
					<filters>
						<filter type="XSLTRANSFORM">
							<param name="fileName">xslt\redi\RediHeader.xslt</param>
							<param name="RediHeaderAction">urn:dowjones:segSample:v1_0/GetSum</param>
						</filter>
					</filters>
				</request>
				<response>
					<messageFormat>STRING</messageFormat>
				</response>
				<transport type="RTS"/><transport type="HTTP"/>
			</transaction>

			<!-- NVP GetSum-->
			<transaction key="Factiva.Gateway.Messages.Segmentation.NVP.V1_0:GetSum">
				<name>MSRVS_REG_SEGSAMPLE_GET_SUM</name>
				<request>
					<messageFormat>XMLSTRING</messageFormat>
				</request>
				<response>
					<messageFormat>XMLSTRING</messageFormat>
					<filters>
						<filter type="XSLTRANSFORM">
							<param name="fileName">xslt\Segmentation\NVPGetSumResponse.xslt</param>
						</filter>
					</filters>
				</response>
				<transport type="RTS"/><transport type="HTTP"/>
			</transaction>

		</transactionSettings>


	</ServiceSettings>
</configuration>
