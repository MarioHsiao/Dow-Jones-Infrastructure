@{
    Layout = null;

    DJ.ScriptRegistry()
        .WithJQueryUIInteractions()
        .WithJQueryUIEffects()
        .Include("~/Content/bootstrap/js/bootstrap.min.js")
        .Include("~/Content/bootstrap/js/google-code-prettify/prettify.js");        

    DJ.StylesheetRegistry()
        .Include("~/Content/bootstrap/css/bootstrap.css")
        .Include("~/Content/bootstrap/css/bootstrap-responsive.css")
        .Include("~/Content/bootstrap/js/google-code-prettify/prettify.css");
}
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>@(ViewBag.Title), using Bootstrap from Twitter</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    @Html.DJ().ScriptRegistry().RenderHead()
    @Html.DJ().StylesheetRegistry().RenderHead()

  </head>

	<body data-spy="scroll" data-target=".subnav" data-offset="50">

		<!-- Navbar
			================================================== -->
		<div class="navbar navbar-fixed-top">
			<div class="navbar-inner">
				<div class="container">
					<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</a>
					@Html.ActionLink("Developer Portal", "Index", "Home", null, new { @class = "brand" })
					<div class="nav-collapse">
						<ul class="nav">
							<li class="dropdown" id="menu1">
								<a class="dropdown-toggle" data-toggle="dropdown" href="#menu1">Components<b class="caret"></b></a>
								<ul class="dropdown-menu">
									<li>@Html.ActionLink("Stock Kiosk", "Index", "StockKiosk")</li>
									<li>@Html.ActionLink("News Radar", "Index", "NewsRadar")</li>
									<li>@Html.ActionLink("Gauge", "Index", "Gauge")</li>
									<li>@Html.ActionLink("Share Chart", "Index", "ShareChart")</li>
									<li>@Html.ActionLink("Article::.. Publication", "Index", "Article")</li>
									<li>@Html.ActionLink("Article::.. Document ", "Document", "Article",new RouteValueDictionary( new {accessionNumber = "MWCO20120608000121"} ), null )</li>
									<li>@Html.ActionLink("Article::.. Multimedia ", "Multimedia", "Article",new RouteValueDictionary( new {accessionNumber = "MMSAVZ0020120612e86c0002v"} ), null )</li>
								</ul>
							</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
		@{
			DJ.ScriptRegistry().OnDocumentReady("$('.dropdown-toggle').dropdown();");
		}
		<div class="container" style="padding-top:40px;">
			<section id="header">
				<div class="page-header"><h1>@(ViewBag.Title)</h1></div>
			</section>
			<div class="row"><span class="span6">@RenderBody()</span></div>
       
            
			@if (IsSectionDefined("SomeContent"))
			{
				<div id="sampleSection" class="row">
					<span class="span12">@RenderSection("SomeContent")</span>
				</div>
			}
		</div><!-- /container -->



		<!-- Le javascript
			================================================== -->
		<!-- Placed at the end of the document so the pages load faster -->
		@Html.DJ().StylesheetRegistry().Render() 
		@Html.DJ().ScriptRegistry().Render()

		<!-- Analytics
			================================================== -->

	</body>
    @RenderSection("scripts", false)

</html>
