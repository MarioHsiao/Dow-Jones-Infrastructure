<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="ServiceSettings" type="Factiva.Gateway.Config.V1_0.ServiceSectionHandler, FactivaGateway"/>
	</configSections>
	<ServiceSettings>
		<transactionSettings>
			<transaction key="Factiva.Gateway.Messages.Metrics.ODS.V1_0:RecordOperationData">
				<name>RecordOperationalEvent</name>
				<request>
					<messageFormat>XMLSTRING</messageFormat>
				</request>
				<response>
					<messageFormat>BINARY</messageFormat>
				</response>
				<transport type="RTS"/><transport type="HTTP"/>
			</transaction>
			<!-- START UER TRANSACTIONS-->
			<transaction key="Factiva.Gateway.Messages.Metrics.UER.V1_0:MetricsData">
				<name>UERRecordEvent_bstr</name>
				<request>
					<messageFormat>XMLSTRING</messageFormat>
				</request>
				<response>
					<messageFormat>STRING</messageFormat>
          <filters>
            <filter type="XSLTRANSFORM">
              <param name="fileName">xslt\Metrics\MetricsDataResponse.xslt</param>
            </filter>
          </filters>
				</response>
				<transport type="RTS"/><transport type="HTTP"/>
			</transaction>

			<transaction key="Factiva.Gateway.Messages.Assets.Core.Metrics.V1_0:UpdateAssestStatistics">
				<name>urn:factiva:assets:Core:Metrics:data:v1_0:Metrics:UpdateAssestStatistics</name>
				<request>
					<messageFormat>STRING</messageFormat>
					<filters>
						<filter type="XSLTRANSFORM">
							<param name="fileName">xslt\Common\addnamespace.xslt</param>
							<param name="namespace">urn:factiva:assets:Core:Metrics:data:v1_0</param>
						</filter>
					</filters>
				</request>
				<response>
					<messageFormat>STRING</messageFormat>
					<filters>
						<filter type="XSLTRANSFORM">
							<param name="fileName">xslt\Common\RemoveNamespace.xslt</param>
						</filter>
					</filters>
				</response>
				<transport type="RTS"/><transport type="HTTP"/>
			</transaction>
			
		</transactionSettings>
	</ServiceSettings>
</configuration>
