@{
    Layout = null;

    DJ.ScriptRegistry()
        .WithJQueryUIInteractions()
        .WithJQueryUIEffects()
        .Include("~/Content/bootstrap/js/bootstrap.js")
        .Include("~/Content/bootstrap/js/google-code-prettify/prettify.js");        

    DJ.StylesheetRegistry()
        .Include("~/Content/bootstrap/css/bootstrap.css")
        .Include("~/Content/bootstrap/css/bootstrap-responsive.css")
        .Include("~/Content/bootstrap/js/google-code-prettify/prettify.css");
}
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>@(ViewBag.Title), Rainwater Newsletter Viewer</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    @Html.DJ().ScriptRegistry().RenderHead()
    @Html.DJ().StylesheetRegistry().RenderHead()

  </head>

  <body data-spy="scroll" data-target=".subnav" data-offset="50">

    <!-- Navbar
    ================================================== -->
    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          @Html.ActionLink("RainWater", "Index", "Newsletter", null, new { @class = "brand" })
          <!--@*
          <div class="nav-collapse">
            <ul class="nav">
                <li class="dropdown" id="menu0">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#menu0">SignalR<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li>@Html.ActionLink("News Feed", "POC", "NewsFeed")</li>
                    </ul>
                </li>
                <li class="dropdown" id="menu1">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#menu1">Components<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li>@Html.ActionLink("Article Viewer", "Index", "Article")</li>
                        <li>@Html.ActionLink("Stock Kiosk", "Index", "StockKiosk")</li>
                    </ul>
                </li>
            </ul>
          </div>
          *@-->
        </div>
      </div>
    </div>
    <div class="container" style="padding-top:40px;">
        @RenderBody()
    </div><!-- /container -->



    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    @Html.DJ().StylesheetRegistry().Render() 
    @Html.DJ().ScriptRegistry().Render()

    <!-- Analytics
    ================================================== -->

</body>
</html>
