@model DateHistogramModel
@ClientPlugin dj_DateHistogram
@ScriptResource RelativeResourceName=DateHistogram.js
@ClientTemplate RelativeResourceName=ClientTemplates.DateHistogramContents.htm, TemplateId=datehistogramContents
@{     
    CssClass = "dj_DateHistogram dj_DateNavigator";
    Html.DJ().ScriptRegistry().WithServiceProxy();
    Html.DJ().ScriptRegistry().WithJQueryUIWidgets();
    Html.DJ().ScriptRegistry().WithPopupBalloon();
    Html.DJ().ScriptRegistry().WithHighCharts();
 }
