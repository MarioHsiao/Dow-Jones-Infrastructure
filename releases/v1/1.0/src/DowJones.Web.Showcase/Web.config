            <add name="PlatformProxy" verb="*" path="PlatformProxy.asmx" type="DowJones.Utilities.Handlers.Proxy.StreamingProxyHandler, DowJones.Infrastructure" />
            <add name="HandlerFactory" verb="*" path="DowJones.Utilities.Handlers.*.axd" type="DowJones.Utilities.Handlers.BasicHttpHandlerFactory, DowJones.Infrastructure" />
