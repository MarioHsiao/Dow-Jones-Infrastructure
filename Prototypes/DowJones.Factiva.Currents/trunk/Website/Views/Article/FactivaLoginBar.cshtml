@using System.Configuration

@{
	var loginUrl = ConfigurationManager.AppSettings.Get("LoginUrl").Trim('/');
    var signUpUrl = ConfigurationManager.AppSettings.Get("SignupUrl").Trim('/');
}
<div class="form-actions">
    <a href="@loginUrl" class="dj_login">Log In</a> | 
    <a href="@signUpUrl" class="dj_signUp">Sign up</a>
</div>