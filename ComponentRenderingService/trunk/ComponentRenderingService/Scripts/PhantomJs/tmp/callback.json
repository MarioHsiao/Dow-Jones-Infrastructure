function(chart) {
	chart.renderer.rect(-5, -5, 20, 20, 5).attr({
		fill : 'white',
		stroke : 'white',
		'stroke-width' : 1,
      'stroke-opacity':2,
      'stroke-width':5,
      'transform':'translate(1,1)',
      'fill-opacity':2,
     
	}).add();
  chart.renderer.rect(chart.spacingBox.width+10, chart.spacingBox.height+10, 20, 20, 5).attr({
		fill : 'white',
		stroke : 'white',
		'stroke-width' : 1,
      'stroke-opacity':2,
      'stroke-width':5,
      'transform':'translate(1,1)',
      'fill-opacity':2,
      
	}).add();
  chart.renderer.rect(chart.spacingBox.width+10,0, 20, 20, 5).attr({
		fill : 'white',
		stroke : 'white',
		'stroke-width' : 1,
      'stroke-opacity':2,
      'stroke-width':5,
      'transform':'translate(1,1)',
      'fill-opacity':2,
     
	}).add();
  chart.renderer.rect(0,chart.spacingBox.height+10, 20, 20, 5).attr({
		fill : 'white',
		stroke : 'white',
		'stroke-width' : 1,
      'stroke-opacity':2,
      'stroke-width':5,
      'transform':'translate(1,1)',
      'fill-opacity':2,
      
	}).add();
}
		